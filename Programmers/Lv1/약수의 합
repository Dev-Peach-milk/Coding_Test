//나의 풀이
function solution(n) {
    var answer = 0;
    if(n <= 1) return n;
    if(n === 2) return 3;

    for(let i = 1; i <= parseInt(Math.sqrt(n)); i++) {

        if(n % i === 0) {
            if(i === Math.sqrt(n)) {
                answer += i
            } else {
            answer += (i + (n / i))
                    console.log(i, n/i, answer)
            }
        }
    }
    return answer;
}
